            "delimiter": ",",
            "keepTime": false,
            "replace": true,
            "source": "labels"
          }
        },
        {
          "id": "configFromData",
          "options": {
            "applyTo": {
              "id": "byName",
              "options": "total_bytes"
            },
            "configRefId": "A",
            "mappings": [
              {
                "fieldName": "max_total_bytes",
                "handlerKey": "max"
              },
              {
                "fieldName": "min_total_bytes",
                "handlerKey": "min"
              }
            ]
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "k": true,
              "max_bytes_per_row": false,
              "max_total_bytes": true,
              "min_bytes_per_row": false,
              "min_total_bytes": true
            },
            "includeByName": {},
            "indexByName": {
              "bloom_bytes": 5,
              "bytes_per_row": 3,
              "field": 0,
              "k": 6,
              "max_bytes_per_row": 7,
              "min_bytes_per_row": 8,
              "rows": 2,
              "total_bytes": 1,
              "values_bytes": 4
            },
            "renameByName": {
              "bloom_bytes": "Bloom filter bytes",
              "bytes_per_row": "Bytes/row",
              "field": "Field",
              "rows": "Rows",
              "total_bytes": "Total bytes",
              "values_bytes": "Value bytes"
            }
          }
        }
      ],
